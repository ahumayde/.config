{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [ {
      "alignment": "left",
      "segments": [ { 
          "type": "os",
          "style": "diamond",
          "background": "#18354c",
          "foreground": "#F9F1A5",
          "template": "{{ .Icon }} ",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0"
      }, {
          "type": "path",
          "style": "diamond",
          "background": "#18354c",
          "foreground": "#3B78FF",
          "template": "\ue5ff {{ .Path }} ",
          "leading_diamond": "<transparent,#18354C>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "properties": { "style": "folder" }
      }, {
          "type": "git",
          "style": "powerline",
          "background": "#18354c",
          "foreground": "#7b44ff",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} ",
          "powerline_symbol": "\ue0b0",
          "properties": { "fetch_upstream_icon": true }
      }, {
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
          "{{ if gt .Ahead 0 }}#29FCBC{{ end }}",
          "{{ if gt .Behind 0 }}#D22A24{{ end }}" ],
          "background": "#18354c",
          "template": "{{if or (gt .Ahead 0) (gt .Behind 0) }}{{ .BranchStatus }}{{ end }}",
          "leading_diamond": "<transparent,#18354C>\ue0b0 </>",
          "trailing_diamond": "\ue0b4",
          "properties": { "fetch_status": true }
      }, {
          "type": "git",
          "style": "diamond",
          "foreground": "#00ff00",
          "background": "#18354c",
          "template": "{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}",
          "leading_diamond": "<transparent,#18354C>\ue0b0 </>",
          "trailing_diamond": "\ue0b4",
          "properties": { "fetch_status": true }
      }, {
          "type": "git",
          "style": "diamond",
          "foreground": "#ff2222",
          "background": "#18354c",
          "template": "{{ if .Working.Changed }} {{ .Working.String }}{{ end }}",
          "leading_diamond": "<transparent,#18354C>\ue0b0 </>",
          "trailing_diamond": "\ue0b4",
          "properties": { "fetch_status": true }
       }, {
          "type": "git",
          "style": "diamond",
          "foreground": "#D75CFF",
          "background": "#18354c",
          "template": "{{ if gt .StashCount 0 }}\ueb4b {{ .StashCount }}{{ end }}",
          "leading_diamond": "<transparent,#18354C>\ue0b0 </>",
          "trailing_diamond": "\ue0b4",
          "properties": { "fetch_stash_count": true }
      }
      ],
      "type": "prompt"
      },
      {
      "alignment": "left",
      "newline": true,
      "segments": [
          {
              "background": "transparent",
              "foreground": "#F9F1A5",
              "leading_diamond": "\ue0b6 ",
              "style": "diamond",
              "template": "<f>\u276f$</f>",
              "trailing_diamond": "\ue0b4",
              "type": "text"
          }
    ],
    "type": "prompt"
}
  ],
  "final_space": true,
  "version": 2
}
